{
    "name": "partit",
    "type": "rootfs",
    "priority": 0,
    "support": [
        "linux/amd64",
        "linux/arm64"
    ],
    "env": {
        "BOOT_LABEL": "BOOT",
        "ROOT_LABEL": "otaroot"
    },
    "hostDeps": [
        "mount",
        "xz-utils",
        "kpartx",
        "e2fsprogs",
        "pipx",
        "lz4"
    ],
    "buildRecipes": [
        "disk.ts"
    ],
    "cleanRecipes": [
        "umount.ts"
    ]
}
